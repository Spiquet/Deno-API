{
    "version": "0.2.0",
    "configurations": [

    
        {
            "name": "Launch Deno",
            "type": "node",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "runtimeExecutable": "deno",
            "runtimeArgs": [
                "run",
                "--inspect",
                "--allow-net",
                "--config",
                "./tsconfig.json",
                "main.ts" 
            ],
            "port": 9229
        }
    ]
}